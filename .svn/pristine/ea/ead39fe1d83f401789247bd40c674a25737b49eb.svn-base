<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:overScrollMode="never"
    android:scrollbars="none"
    android:fillViewport="true">
<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingTop="50dp">

    <ImageView
        android:id="@+id/iv_logo"
        android:layout_width="72dp"
        android:layout_height="72dp"
        android:layout_marginTop="15dp"
        android:src="@mipmap/logo_lieying"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.047"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.051" />

    <TextView
        android:id="@+id/tv_username"
        android:layout_width="129dp"
        android:layout_height="25dp"
        android:text="测试号"
        android:gravity="left|center_vertical"
        android:layout_marginTop="15dp"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.382"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.048" />

    <TextView
        android:id="@+id/tv_id"
        android:layout_width="129dp"
        android:layout_height="25dp"
        android:gravity="left|center_vertical"
        android:text="ID:13112345678"
        app:layout_constraintHorizontal_bias="0.312"
        app:layout_constraintStart_toStartOf="@id/tv_username"
        app:layout_constraintTop_toBottomOf="@+id/tv_username" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="36dp"
        android:layout_height="25dp"
        android:gravity="left|center_vertical"
        android:text="身份:"
        app:layout_constraintHorizontal_bias="0.234"
        app:layout_constraintStart_toStartOf="@id/tv_id"
        app:layout_constraintTop_toBottomOf="@+id/tv_id" />

    <TextView
        android:id="@+id/tv_usertype"
        android:layout_width="60dp"
        android:layout_height="25dp"
        android:gravity="center_vertical"
        android:text="经纪人"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/tv_id" />

    <TextView
        android:id="@+id/tv_label_heart"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:text="爱心值:"
        android:gravity="center_vertical"
        app:layout_constraintHorizontal_bias="0.597"
        app:layout_constraintStart_toEndOf="@id/tv_usertype"
        app:layout_constraintTop_toTopOf="@id/tv_usertype"
        app:layout_constraintVertical_bias="0.117" />

    <TextView
        android:id="@+id/tv_heart_value"
        android:layout_width="70dp"
        android:layout_height="25dp"
        android:gravity="center"
        android:text="131"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@id/tv_label_heart"
        app:layout_constraintHorizontal_bias="0.774"
        app:layout_constraintTop_toTopOf="@id/tv_label_heart"
        app:layout_constraintVertical_bias="0.118" />

    <ImageView
        android:id="@+id/iv_toright"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_toright"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.958"
        app:layout_constraintTop_toTopOf="@id/tv_id"
        app:layout_constraintVertical_bias="0.082" />

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/colorGray"
        android:layout_marginTop="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf ="@id/tv_heart_value"
        app:layout_constraintVertical_bias="0.183" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="88dp"
        android:layout_height="25dp"
        android:gravity="center"
        android:text="余额"
        android:textSize="15sp"
        android:layout_marginTop="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.111"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view"
        app:layout_constraintVertical_bias="0.205" />

    <TextView
        android:id="@+id/tv_balance"
        android:layout_width="88dp"
        android:layout_height="25dp"
        android:gravity="center"
        android:text="1200"
        android:textSize="15sp"
        android:textStyle="bold"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.111"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textView4"
        app:layout_constraintVertical_bias="0.256" />


    <TextView
        android:id="@+id/textView3"
        android:layout_width="88dp"
        android:layout_height="25dp"
        android:gravity="center"
        android:text="红包"
        android:textSize="15sp"
        android:layout_marginTop="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.544"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view" />

    <TextView
        android:id="@+id/tv_tax"
        android:layout_width="88dp"
        android:layout_height="25dp"
        android:gravity="center"
        android:text="1200"
        android:textSize="15sp"
        android:textStyle="bold"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.544"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintVertical_bias="0.256" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="88dp"
        android:layout_height="25dp"
        android:gravity="center"
        android:text="影贝"
        android:textSize="15sp"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@id/view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintVertical_bias="0.205" />

    <TextView
        android:id="@+id/tv_film_money"
        android:layout_width="88dp"
        android:layout_height="25dp"
        android:gravity="center"
        android:text="1200"
        android:layout_marginTop="10dp"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@id/textView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/view2"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_marginTop="20dp"
        android:background="@color/colorGray"
        app:layout_constraintTop_toBottomOf="@id/tv_film_money"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <ImageView
        android:id="@+id/iv_banner"
        android:layout_width="343dp"
        android:layout_height="75dp"
        android:src="@mipmap/banner"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view2"/>

    <TextView
        android:layout_width="81dp"
        android:layout_height="23dp"
        android:layout_marginTop="20dp"
        android:text="我的订单"
        android:textColor="#ff414141"
        android:textSize="15sp"
        android:gravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.103"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/iv_banner"
        app:layout_constraintVertical_bias="0.54" />

    <TextView
        android:id="@+id/tv_label_all_order"
        android:layout_width="58dp"
        android:layout_height="18dp"
        android:text="全部订单"
        android:textColor="#ff666666"
        android:textSize="12sp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.903"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/iv_banner"
        app:layout_constraintVertical_bias="0.543" />

    <View
        android:id="@+id/view5"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/colorGray"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/tv_label_all_order"
        app:layout_constraintVertical_bias="0.571"
        tools:layout_editor_absoluteX="-16dp" />

    <include
        android:id="@+id/include"
        layout="@layout/layout_my_order"
        android:layout_width="409dp"
        android:layout_height="75dp"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toBottomOf="@id/view5"
        app:layout_constraintVertical_bias="0.644"
        tools:layout_editor_absoluteX="2dp" />

    <TextView
        android:id="@+id/tv_label_my_hotbuy"
        android:layout_width="81dp"
        android:layout_height="23dp"
        android:gravity="center"
        android:text="我的抢购"
        android:textColor="#ff414141"
        android:textSize="15sp"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.103"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/include"
        app:layout_constraintVertical_bias="0.732" />

    <View
        android:id="@+id/view6"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/colorGray"
        app:layout_constraintTop_toBottomOf="@id/tv_label_my_hotbuy"
        app:layout_constraintVertical_bias="0.762"
        tools:layout_editor_absoluteX="2dp" />

    <include
        android:id="@+id/lmt"
        layout="@layout/layout_my_hotbuy"
        android:layout_width="413dp"
        android:layout_height="81dp"
        android:layout_marginTop="45dp"
        app:layout_constraintTop_toBottomOf="@+id/view6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintVertical_bias="0.882" />

    <View
        android:id="@+id/view3"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="@color/colorGray"
        app:layout_constraintTop_toBottomOf="@+id/lmt"
        app:layout_constraintVertical_bias="0.916"
        tools:layout_editor_absoluteX="2dp" />

    <ImageView
        android:id="@+id/iv_banner1"
        android:layout_width="343dp"
        android:layout_height="75dp"
        android:src="@mipmap/banner1"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/view3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/view4"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="@color/colorGray"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/iv_banner1"
        app:layout_constraintVertical_bias="0.916"
        tools:layout_editor_absoluteX="2dp" />

    <include layout="@layout/layout_setting"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      app:layout_constraintTop_toBottomOf="@+id/view4"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      />

</androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>